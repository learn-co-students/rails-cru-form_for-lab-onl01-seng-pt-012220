<h1>Song Form</h1>

<%= form_for(@song) do |f| %>
    <%= f.label 'name' %><br>
    <%= f.text_field :name %><br>

    <%= f.label 'artist id' %><br>
    <%= f.text_field :artist_id %><br>

    <%= f.label 'genre id' %><br>
    <%= f.text_field :genre_id %><br>

    <%= f.submit 'Create Song'%>
<% end %>
